%table.display.table-bordered.table
  %thead
    %tr
      %th Scheme Name
      %th 
      %th
      %th
  %tbody
    - for grade_scheme in @grade_schemes
      %tr
        //%td= check_box_tag "grade_scheme_ids[]", grade_scheme.id
        %td= grade_scheme.scheme_name
        %td= link_to 'View', grade_scheme
        %td= link_to 'Edit', edit_grade_scheme_path(grade_scheme)
        %td= link_to 'Destroy', grade_scheme, :confirm => 'Are you sure?', :method => :delete

%div#footerNav
  = link_to 'New Grade scheme', new_grade_scheme_path, {:class => 'btn btn-primary'}
